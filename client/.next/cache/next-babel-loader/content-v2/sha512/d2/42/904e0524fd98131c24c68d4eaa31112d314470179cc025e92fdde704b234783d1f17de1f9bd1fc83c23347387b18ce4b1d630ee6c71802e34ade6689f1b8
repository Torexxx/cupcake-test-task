irst').then((res) =>\n        setFirst(res.data.rates),\n      );\n    }, 2000);\n  }, [first]);\n  useEffect(() => {\n    setTimeout(() => {\n      axios('http://localhost:5000/api/v1/second').then((res) =>\n        setSecond(res.data.rates),\n      );\n    }, 2000);\n  }, [second]);\n  useEffect(() => {\n    setTimeout(() => {\n      axios('http://localhost:5000/api/v1/third').then((res) =>\n        setThird(res.data.rates),\n      );\n    }, 2000);\n  }, [third]);\n\n\n\n  return <div className={styles.wrapper}>\n      <table className={styles.respTab}>\n      <thead>\n      <tr>\n        <th>Заголовок Pair name/market</th>\n        <th>First</th>\n        <th>Second</th>\n        <th>Third</th>\n      </tr>\n      </thead>\n      <tbody>\n      <tr>\n        <td><span>Заголовок 1</span>RUB/CUPCAKE  </td>\n        <td><span>Заголовок 2</span>Контент 1</td>\n        <td><span>Заголовок 3</span>Контент 1</td>\n        <td><span>Заголовок 4</span>Контент 1</td>\n      </tr>\n      <tr>\n        <td><span>Заголовок 1</span>USD/CUPCAKE</td>\n        <td><span>Заголовок 2</span>Контент 1</td>\n        <td><span>Заголовок 3</span>Контент 1</td>\n        <td><span>Заголовок 4</span>Контент 1</td>\n      </tr>\n      <tr>\n        <td><span>Заголовок 1</span>EUR/CUPCAKE</td>\n        <td><span>Заголовок 2</span>Контент 1</td>\n        <td><span>Заголовок 3</span>Контент 1</td>\n        <td><span>Заголовок 4</span>Контент 1</td>\n      </tr>\n      <tr>\n        <td><span>Заголовок 1</span>RUB/USD</td>\n        <td>{(first.RUB / first.USD).toFixed(2) }</td>\n        <td>{(second.RUB / second.USD).toFixed(2) }</td>\n        <td>{(third.RUB / third.USD).toFixed(2) }</td>\n      </tr>\n      <tr>\n        <td><span>Заголовок 1</span>RUB/EUR</td>\n        <td>{(first.RUB / first.EUR).toFixed(2) }</td>\n        <td>{(second.RUB / second.EUR).toFixed(2) }</td>\n        <td>{(third.RUB / third.EUR).toFixed(2) }</td>\n      </tr>\n      <tr>\n        <td><span>Заголовок 1</span>EUR/USD</td>\n        <td>{(first.EUR / first.USD).toFixed(2) }</td>\n        <td>{(second.EUR / second.USD).toFixed(2) }</td>\n        <td>{calc(third, 'EUR', 'USD') && 'HELLO'}</td>\n      </tr>\n      </tbody>\n    </table>\n\n  </div>;\n};\n\nexport default Index;\n"]},"metadata":{},"sourceType":"module"}